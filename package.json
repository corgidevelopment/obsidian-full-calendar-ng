        "fix-lint": "prettier --config .prettierrc.json --write src/**/*.ts*",
        "compile": "tsc -noEmit --skipLibCheck",
        "prepare": "husky install",
        "test": "jest --ci --silent=true",
        "test-update": "jest --silent=false --updateSnapshot",
        "test-dev": "jest --watch",
        "coverage": "jest --coverage"
    },
